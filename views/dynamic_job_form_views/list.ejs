<html>
  <head>
    <style>
      .container{
        display:flex;
        flex-direction:column;
        justify-content: center;
        align-items: center;
      }
      table,tr,td,th{
        border:2px solid black;
        border-collapse: collapse;
      }
    </style>
  </head>
  <body>
        <div class="container">
            <table>
                <tr>
                   <th>First Name</th>
                   <th>Last Name</th>
                   <th>Email</th>
                   <th>Design</th>
                   <th>Gender</th>
                   <th>City</th>
                   <th>State</th>
                   <th>Update</th>
                </tr>
                <%result.forEach((element)=>{%>
                  <tr>
                    <td><%=element.first_name%></td>
                    <td><%=element.last_name%></td>
                    <td><%=element.email%></td>
                    <td><%=element.design%></td>
                    <td><%=element.gender%></td>
                    <td><%=element.city%></td>
                    <td><%=element.state%></td>
                    <td><a href="/update_link?id=<%=element.candidate_id%>">Update</a></td>
                  </tr>  
                <%})%>
            </table>
        </div>  
  </body>
  </html>